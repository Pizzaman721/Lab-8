#Project Euler 
import math


def divisors(n):# creates a list of divisors, will later check if 500 divisos
    listofdivisors=[]
    limit=int(math.sqrt(n))
    for a in range(1,limit+1,1):
        if n%a==0:
            listofdivisors.append(a)
            if a!=n/a:
                listofdivisors.append(n/a)
    return len(listofdivisors)

def TN(n): #checks if number is a triangle number
    m=int(math.sqrt(n*2))
    return m*(m+1)/2==n
